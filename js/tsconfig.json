{
  "compilerOptions": {
    "outFile": "tetris.js",
    "target": "es2015",                                  
    "lib": ["es2015", "dom"],
    "module": "amd",
    "resolveJsonModule": false,
    "allowJs": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
