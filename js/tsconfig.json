{
  "compilerOptions": {
    "target": "es6",                                  
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
  }
}
